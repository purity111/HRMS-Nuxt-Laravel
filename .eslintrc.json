{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "semi": [
      "error",
      "always"
    ],
    "no-param-reassign": "off",
    "no-console": "off",
    "max-len": "off"
  }
}
