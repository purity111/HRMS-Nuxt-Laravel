{
  "extends": "airbnb/base",
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "no-param-reassign": 0,
    "prefer-rest-params": 0,
    "no-plusplus": 0,
    "prefer-destructuring": 0,
    "no-shadow": 0,
    "no-console": 0,
    "global-require": 0
  }
}
